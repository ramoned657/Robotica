@article{ActuatorFaulDetection,
	author = {Julio Alberto Guzmán-Rabasa and Francisco Ronay López-Estrada and Brian Manuel González-Contreras and Guillermo Valencia-Palomo and Mohammed Chadli and Madain Pérez-Patricio},
	title ={Actuator fault detection and isolation on a quadrotor unmanned aerial vehicle modeled as a linear parameter-varying system},
	journal = {Measurement and Control},
	volume = {52},
	number = {9-10},
	pages = {1228-1239},
	year = {2019},
	doi = {10.1177/0020294018824764},
	
	URL = { 
	https://doi.org/10.1177/0020294018824764
	
	},
	eprint = { 
	https://doi.org/10.1177/0020294018824764
	
	}
	,
	abstract = { This paper presents the design of a fault detection and diagnosis system for a quadrotor unmanned aerial vehicle under partial or total actuator fault. In order to control the quadrotor, the dynamic system is divided in two subsystems driven by the translational and the rotational dynamics, where the rotational subsystem is based on a linear parameter-varying model. A robust linear parameter-varying observer applied to the rotational subsystem is considered to detect actuator faults, which can occur as total failures (loss of a propeller or a motor) or partial faults (degradation). Furthermore, fault diagnosis is done by analyzing the displacements of the roll and pitch angles. Numerical experiments are carried out in order to illustrate the effectiveness of the proposed methodology. }
}
