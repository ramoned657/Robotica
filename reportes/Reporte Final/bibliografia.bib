	journal = {Electronics (Switzerland)},
	keywords = {Countermea-sures,Drone,Electromagnetic interference,Extra high voltage (EHV),High voltage direct current (HVDC),Influence,Power converter station,Ultra high voltage (UHV),Unmanned aerial vehicle (UAV)},
	month = {3},
